<h1> New user </h1>
<%= form_for @user do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %>
  <p>
  <%= f.label :height %><br>
  <%= f.text_field :height %>
  <p>
  <%= f.label :happiness %><br>
  <%= f.text_field :happiness %>
  <p>
  <%= f.label :nausea %><br>
  <%= f.text_field :nausea %>
  <p>
  <%= f.label :tickets %><br>
  <%= f.text_field :tickets %>
  <p>
  <%= f.label :password %><br>
  <%= f.text_field :password %>
  <p>
  <%= f.label :admin %><br>
  <%= f.check_box :admin %>
  <p>
  <%= f.submit %>
<% end %>
